                                        </DoubleAnimationUsingKeyFrames>
                                        <ColorAnimationUsingKeyFrames Duration="00:00:00.001" Storyboard.TargetName="WhiteRightSelected" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
